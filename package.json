{
  "name": "PSone.css",
  "version": "1.0.1",
  "description": "Playstation 1 style CSS Framework, inspired by NES.css",
  "main": "index.html",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "dev": "yarn poi index.html --serve ",
    "build:main": "npm run build:autoprefix && npm run build:cleancss",
    "build:autoprefix": "postcss --use autoprefixer --map false --output PSone.css PSone.css",
    "build:cleancss": "cleancss -o PSone.min.css PSone.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belthesar/PSone.css.git"
  },
  "keywords": [
    "css",
    "psone",
    "css-framework"
  ],
  "author": "98mprice",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/belthesar/PSone.css/issues"
  },
  "homepage": "https://github.com/belthesar/PSone.css#readme",
  "browserslist": [
    "> 1%"
  ],
  "devDependencies": {
    "@storybook/addon-backgrounds": "^7.4.5",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/html": "^7.4.5",
    "autoprefixer": "^10.4.16",
    "clean-css-cli": "^5.6.2",
    "poi": "^12.5.5",
    "postcss-cli": "^10.1.0"
  },
  "dependencies": {
    "postcss": "^8.4.30"
  }
}
